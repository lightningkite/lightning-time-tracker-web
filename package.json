{
  "name": "lk-react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "npm run build:prod",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "deploy": "export AWS_PROFILE=lk || set AWS_PROFILE=lk && npm run build && terraform -chdir=terraform/lightningkite init && terraform -chdir=terraform/lightningkite apply -auto-approve",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@lightningkite/lightning-server-simplified": "^1.0.0-alpha.3",
    "@lightningkite/mui-lightning-components": "^1.0.0-alpha.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/lab": "^5.0.0-alpha.120",
    "@mui/material": "^5.11.10",
    "@mui/x-data-grid": "^5.17.24",
    "@mui/x-date-pickers": "^5.0.19",
    "@ngneat/falso": "^6.4.0",
    "dayjs": "^1.11.7",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rollup-plugin-analyzer": "^4.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
